{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"indent": "off",
		"operator-linebreak" : "off",
		"no-trailing-spaces": "off",
		"object-curly-spacing": "off"
    },
    "extends": ["eslint:recommended", "google"]
}